{
  "name": "@hyponos/back",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "18.x.x"
  },
  "scripts": {
    "build": "node_modules/.bin/lb-tsc",
    "build:watch": "node_modules/.bin/lb-tsc --watch",
    "lint": "node_modules/.bin/eslint .",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:fix": "npm run prettier:cli --write",
    "pretest": "npm run rebuild",
    "test": "nyc node_modules/.bin/lb-mocha --allow-console-logs \"dist/__tests__\"",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate --rebuild",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "PORT=4000 node -r source-map-support/register .",
    "start:watch": "node_modules/.bin/tsc-watch --target es2017 --outDir ./dist --onSuccess \"npm run start\"",
    "clean": "node_modules/.bin/lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "author": "Lucas Hubert <hello@lucas-hubert.me>",
  "dependencies": {
    "@loopback/authentication": "~9.0.10",
    "@loopback/authentication-jwt": "~0.12.10",
    "@loopback/authorization": "~0.12.10",
    "@loopback/boot": "~5.0.10",
    "@loopback/build": "~9.0.10",
    "@loopback/core": "~4.0.10",
    "@loopback/eslint-config": "~14.0.5",
    "@loopback/repository": "~5.1.5",
    "@loopback/rest": "~12.0.10",
    "@loopback/rest-explorer": "~5.0.10",
    "@loopback/security": "~0.8.10",
    "@loopback/service-proxy": "~5.0.10",
    "@loopback/testlab": "~6.1.5",
    "@types/multer": "~1.4.13",
    "@types/nodemailer": "~6.4.21",
    "ajv": "~8.11.2",
    "bcryptjs": "~2.4.3",
    "crypto": "~1.0.1",
    "date-fns": "~2.28.0",
    "debug": "~4.3.7",
    "dotenv": "~16.0.3",
    "isemail": "~3.2.0",
    "jsonwebtoken": "~9.0.2",
    "lodash": "~4.17.21",
    "loopback-connector-postgresql": "~5.5.2",
    "mime": "~4.1.0",
    "multer": "~2.0.2",
    "node-cleanup": "~2.1.2",
    "nodemailer": "~7.0.10",
    "tsc-watch": "~5.0.3",
    "tslib": "~2.3.1",
    "yaml": "~2.8.1"
  },
  "devDependencies": {
    "@types/mime": "~3.0.4",
    "@types/node": "~12.20.55",
    "@typescript-eslint/eslint-plugin": "~5.62.0",
    "@typescript-eslint/parser": "~5.62.0",
    "eslint": "~8.57.1",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-eslint-plugin": "~4.1.0",
    "eslint-plugin-jest": "~26.1.5",
    "eslint-plugin-mocha": "~10.0.5",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-react": "~7.29.4",
    "nyc": "~15.1.0",
    "prettier": "~2.6.2",
    "source-map-support": "~0.5.21",
    "typescript": "~4.9.5"
  }
}
