{
  "name": "hyponos-back",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "node_modules/.bin/lb-tsc",
    "build:watch": "node_modules/.bin/lb-tsc --watch",
    "lint": "node_modules/.bin/eslint .",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:fix": "npm run prettier:cli --write",
    "eslint": "node_modules/.bin/lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run node_modules/.bin/eslint --fix",
    "pretest": "npm run rebuild",
    "test": "nyc node_modules/.bin/lb-mocha --allow-console-logs \"dist/__tests__\"",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "PORT=4000 node -r source-map-support/register .",
    "start:watch": "node_modules/.bin/tsc-watch --target es2017 --outDir ./dist --onSuccess \"npm run start\"",
    "clean": "node_modules/.bin/lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "heroku-postbuild": "node_modules/.bin/lb-clean dist *.tsbuildinfo .eslintcache && node_modules/.bin/lb-tsc"
  },
  "author": "Lucas Hubert <hubertlucas41@gmail.com>",
  "dependencies": {
    "@loopback/authentication": "^8.1.1",
    "@loopback/authentication-jwt": "^0.11.1",
    "@loopback/authorization": "^0.11.1",
    "@loopback/boot": "^4.1.1",
    "@loopback/core": "^3.1.1",
    "@loopback/repository": "^4.1.1",
    "@loopback/rest": "^11.1.1",
    "@loopback/rest-explorer": "^4.1.1",
    "@loopback/security": "^0.7.1",
    "@loopback/service-proxy": "^4.1.1",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "loopback-connector-mongodb": "^6.1.0",
    "node-cleanup": "^2.1.2",
    "tsc-watch": "^5.0.3",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.1",
    "@loopback/eslint-config": "^12.0.3",
    "@loopback/testlab": "^4.1.1",
    "@types/node": "^12.20.46",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-mocha": "^10.0.3",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "typescript": "~4.5.5"
  }
}
