{
  "name": "@hyponos/back",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "node_modules/.bin/lb-tsc",
    "build:watch": "node_modules/.bin/lb-tsc --watch",
    "lint": "node_modules/.bin/eslint .",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:fix": "npm run prettier:cli --write",
    "pretest": "npm run rebuild",
    "test": "nyc node_modules/.bin/lb-mocha --allow-console-logs \"dist/__tests__\"",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate --rebuild",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "PORT=4000 node -r source-map-support/register .",
    "start:watch": "node_modules/.bin/tsc-watch --target es2017 --outDir ./dist --onSuccess \"npm run start\"",
    "clean": "node_modules/.bin/lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "heroku-postbuild": "node_modules/.bin/lb-clean dist *.tsbuildinfo .eslintcache && node_modules/.bin/lb-tsc && node ./dist/migrate"
  },
  "author": "Lucas Hubert <hubertlucas41@gmail.com>",
  "dependencies": {
    "@loopback/authentication": "^8.1.2",
    "@loopback/authentication-jwt": "^0.11.2",
    "@loopback/authorization": "^0.11.2",
    "@loopback/boot": "^4.1.2",
    "@loopback/build": "^8.1.2",
    "@loopback/core": "^3.1.2",
    "@loopback/eslint-config": "^12.0.4",
    "@loopback/repository": "^4.1.2",
    "@loopback/rest": "^11.1.2",
    "@loopback/rest-explorer": "^4.1.2",
    "@loopback/security": "^0.7.2",
    "@loopback/service-proxy": "^4.1.2",
    "@loopback/testlab": "^4.1.2",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "ajv": "^8.11.0",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "debug": "^4.3.4",
    "dotenv": "^16.0.0",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "loopback-connector-postgresql": "^5.5.0",
    "multer": "^1.4.4",
    "node-cleanup": "^2.1.2",
    "nodemailer": "^6.7.3",
    "tsc-watch": "^5.0.3",
    "tslib": "^2.3.1",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.20.46",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-mocha": "^10.0.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "typescript": "~4.6.3"
  }
}
