      "^@Utils(.+)": "<rootDir>/src/utils/$1",
      "^@Components(.+)": "<rootDir>/components/$1",
      "^@Interfaces(.+)": "<rootDir>/interfaces/$1"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint"
    ]
  },
  "overrides": {
    "@types/react-transition-group": {
      "@types/react": "17.0.44"
